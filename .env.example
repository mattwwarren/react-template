# Example environment variables
# Copy this to .env.local for local overrides (not committed to git)

# API URL for backend connection
VITE_API_URL=http://localhost:8000

# Enable MSW mocks (set to "true" to use mock data instead of real API)
VITE_USE_MOCKS=false

# App environment (development, production, test)
VITE_APP_ENV=development
