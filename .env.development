# Development environment variables
# These are loaded by Vite during development

# API URL for backend connection
# In development, Vite proxies /api requests to this URL
VITE_API_URL=http://localhost:8000

# Enable MSW mocks (set to "true" to use mock data instead of real API)
VITE_USE_MOCKS=false

# App environment
VITE_APP_ENV=development

# Authentication provider (mock, ory, auth0, keycloak, cognito)
VITE_AUTH_PROVIDER=mock
